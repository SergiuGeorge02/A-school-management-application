<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: black;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.project.FileApplicationController">
   <children>
      <Label alignment="CENTER" layoutY="36.0" prefHeight="17.0" prefWidth="600.0" text="FIPS univeristy application" textFill="#6f6f94">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <Button fx:id="addButton" layoutX="230.0" layoutY="104.0" mnemonicParsing="false" onAction="#add" prefHeight="44.0" prefWidth="141.0" style="-fx-background-color: #6f6f94;" text="Add" textAlignment="CENTER" textFill="WHITE">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <Button fx:id="deleteButton" layoutX="230.0" layoutY="167.0" mnemonicParsing="false" onAction="#delete" prefHeight="44.0" prefWidth="141.0" style="-fx-background-color: #6f6f94;" text="Delete" textAlignment="CENTER" textFill="WHITE">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <Button fx:id="searchButton" layoutX="233.0" layoutY="232.0" mnemonicParsing="false" onAction="#search" prefHeight="44.0" prefWidth="141.0" style="-fx-background-color: #6f6f94;" text="Search" textAlignment="CENTER" textFill="WHITE">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <Button fx:id="departmentsButton" layoutX="233.0" layoutY="299.0" mnemonicParsing="false" onAction="#departments" style="-fx-background-color: #6f6f94;" text="Departments" textAlignment="CENTER" textFill="WHITE">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
